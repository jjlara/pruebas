
<h1>Correo electronico</h1>
<%=  form_tag("") do %>
     <%=  label_tag "nombre", "Nombre" %>
     <%=  text_field_tag :nombre %>
    
     <%=  label_tag "email", "Email" %>
     <%=  email_field_tag :email %>

     <%=  label_tag "asunto", "Asunto" %>
     <%=  text_field_tag :asunto %>

     <%=  label_tag "descripcion", "Descripcion" %>
     <%=  text_area_tag :descripcion,nil, :size => "25x10" %>

     <%=  submit_tag "Enviar" %> 

   
<% end %>






